            "field_column_body": [
              {
                "value": "<p>Examples of your cost:</p>\r\n\r\n<ul>\r\n\t<li>A $6 trip will cost you $5.25&nbsp;and the MBTA will pay $0.75</li>\r\n\t<li>A $20 trip will cost you $5.25 and the MBTA will pay $14.75</li>\r\n\t<li>A $50&nbsp;trip will cost you $10&nbsp;and the MBTA will pay $40</li>\r\n</ul>\r\n",
                "format": "basic_html"
