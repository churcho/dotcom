      "field_custom_html_body": [
        {
          "value": "<hr \/>\r\n<h3 id=\"custom-html\">Custom HTML<\/h3>\r\n\r\n<p>This paragraph provides the usual WYSIWYG editor (see the <a href=\"\/cms\/style-guide\">Style Guide<\/a>&nbsp;for tips on using the editor). Its main purpose is to intersperse content between other types of paragraphs.<\/p>\r\n",
          "format": "full_html"
        }
