<div class="schedule-list-group-item">
  <div class="col-xs-6">
    <%= display_scheduled_prediction(@departure) %>
  </div>
  <div class="col-xs-3">
    <%= if @arrival do %>
      <%= display_scheduled_prediction(@arrival)%>
    <% else %>
      <%= if assigns[:trip] do %>
        <%= @trip.headsign %>
      <% end %>
    <% end %>
  </div>
  <div class="col-xs-3">
    <%= display_alerts(@alerts) %>
  </div>
</div>
