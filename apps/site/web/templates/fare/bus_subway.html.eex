<div class="card card-section">
  <div class="card-block card-block-no-header ">
    <h1><%= @mode_name %> Fares</h1>
    <%= unless @selected_filter.fares == [] do %>
      <%= render "_fares_display.html",
          selected_filter: @selected_filter,
          filters: @filters,
          conn: @conn,
          fare_type: @fare_type
      %>
    <% end %>
  </div>
</div>
